<li id="<%= feed_item.id %>">
  
  <span class="content"><%= link_to feed_item.document_file_name, feed_item.document.url %>
  </span>
  <span class="timestamp">
    - Posted <%= time_ago_in_words(feed_item.created_at) %> ago.
  </span>
  <span class="user">
    by <%= feed_item.user.name %>
  </span>
</li>